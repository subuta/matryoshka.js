{
  "presets": [
    "es2015",
    "stage-2"
  ],
  "plugins": [
    "add-module-exports",
    "transform-object-rest-spread",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    [
      "module-resolver",
      {
        "root": [
          "./"
        ]
      }
    ]
  ]
}
