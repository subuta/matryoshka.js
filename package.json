{
  "name": "matryoshka.js",
  "version": "1.0.0",
  "description": "compose itself like a matriochka",
  "main": "index.js",
  "scripts": {
    "build": "NODE_PATH=./ browserifyinc webapp/index.js -t babelify --extensions=.js -v | NODE_ENV=production loose-envify | uglifyjs --compress 'dead_code, drop_console' > ./public/bundle.js",
    "watch": "NODE_PATH=./ watchify webapp/index.js -t [ babelify ] -p [ browserify-hmr -h 0.0.0.0 ] --extensions=.js -o 'NODE_ENV=development loose-envify | exorcist ./public/bundle.js.map > ./public/bundle.js' -v -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/matryoshka.js.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/matryoshka.js/issues"
  },
  "quokka": {
    "babel": true,
    "env": {
      "params": {
        "env": "NODE_PATH=./"
      }
    }
  },
  "homepage": "https://github.com/subuta/matryoshka.js#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-hmr": "^0.3.5",
    "browserify-incremental": "^3.1.1",
    "electron": "^1.6.11",
    "exorcist": "^0.4.0",
    "loose-envify": "^1.3.1",
    "standard": "^10.0.2",
    "uglify-js": "^3.0.4",
    "wallabify": "0.0.15",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
