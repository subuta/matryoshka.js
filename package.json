{
  "name": "matryoshka.js",
  "version": "0.1.0",
  "description": "Generate your file like a Matryoshka",
  "main": "mat.js",
  "scripts": {
    "test": "nyc ava",
    "watch": "backpack dev",
    "build": "backpack build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/matryoshka.js.git"
  },
  "author": "subuta <shunta.3110@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/matryoshka.js/issues"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "source": [
      "lib/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "require": "babel-register",
    "babel": {
      "extends": ".babelrc",
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "./"
            ],
            "alias": {
              "test": "./test"
            }
          }
        ]
      ]
    }
  },
  "quokka": {
    "babel": true,
    "env": {
      "params": {
        "env": "NODE_PATH=./"
      }
    }
  },
  "homepage": "https://github.com/subuta/matryoshka.js#readme",
  "bin": {
    "mat": "./dist/mat.js"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "backpack-core": "^0.4.3",
    "nyc": "^11.3.0",
    "power-assert": "^1.4.4",
    "proxyquire": "^1.8.0",
    "sinon": "^4.1.2",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "commander": "^2.12.2",
    "globby": "^6.1.0",
    "icepick": "^2.2.0",
    "import-fresh": "^2.0.0",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "sane": "^2.2.0",
    "treeify": "^1.0.1"
  }
}
